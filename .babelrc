{
  "presets": ["react", "es2015", "stage-0", "es2017"],
  "plugins": [
    "transform-object-rest-spread",
    "transform-react-display-name",
    "transform-runtime",
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "react": "preact-compat",
        "react-dom": "preact-compat",
        "client": "./client"
      }
    }]
  ]
}